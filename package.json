{
  "name": "sacandaga",
  "version": "1.0.0",
  "description": "Calendar app for coordinating stays at our family lake house.",
  "author": "Chris Leveille, Michael Leveille",
  "license": "MIT",
  "type": "module",
  "module": "src/server/index.ts",
  "scripts": {
    "prepare": "bun run lefthook.ts",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "tsc": "tsc -p tsconfig.json",
    "build": "bunx --bun vite build && bun run src/server/helpers/workbox.ts",
    "compile": "bun build --compile --minify --sourcemap --bytecode --target bun --outfile main src/server/index.ts ",
    "dev": "concurrently --prefix \"[{name}]\" --names \"server,client\" -c \"bgBlue.bold,bgGreen.bold\" \"bun run --hot src/server/index.ts\" \"bunx --bun vite\"",
    "start": "bun run src/server/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.5",
    "@evilmartians/lefthook": "^1.11.13",
    "@types/bun": "latest",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "concurrently": "^9.1.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.0.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-build": "^7.3.0"
  },
  "dependencies": {
    "@elysiajs/static": "^1.3.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@mui/icons-material": "^7.1.1",
    "@mui/joy": "^5.0.0-beta.52",
    "@mui/material": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.2",
    "@tanstack/react-form": "^1.12.2",
    "@tanstack/react-query": "^5.77.2",
    "dayjs": "^1.11.13",
    "elysia": "^1.3.3",
    "elysia-helmet": "^3.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1",
    "zod": "^3.25.47"
  }
}
