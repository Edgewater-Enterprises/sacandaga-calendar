{
  "name": "sacandaga",
  "version": "1.0.0",
  "description": "Calendar app for coordinating stays at our family lake house.",
  "author": "Chris Leveille, Michael Leveille",
  "license": "MIT",
  "type": "module",
  "module": "src/server/index.ts",
  "scripts": {
    "prepare": "bun run lefthook.ts",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "tsc": "tsc -p tsconfig.json",
    "concurrent": "concurrently --prefix \"[{name}]\" -c \"bgBlue.bold,bgGreen.bold\"",
    "dev": "bun run concurrent --names \"server,client\" \"bun run --hot src/server/index.ts\" \"bunx --bun vite\"",
    "compile": "bun build --compile --minify --sourcemap --bytecode --target bun --outfile main src/server/index.ts",
    "bundle": "bunx --bun vite build && bun run src/client/helpers/workbox.ts",
    "build": "bun run compile && bun run bundle",
    "start": "bun run src/server/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@evilmartians/lefthook": "^1.11.14",
    "@tanstack/react-router-devtools": "^1.123.2",
    "@tanstack/router-plugin": "^1.123.2",
    "@types/bun": "^1.2.17",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "concurrently": "^9.2.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-static-copy": "^3.1.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-build": "^7.3.0"
  },
  "dependencies": {
    "@elysiajs/static": "^1.3.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@mui/icons-material": "^7.2.0",
    "@mui/joy": "^5.0.0-beta.52",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.6.0",
    "@tanstack/react-form": "^1.12.4",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-router": "^1.123.2",
    "dayjs": "^1.11.13",
    "elysia": "^1.3.5",
    "elysia-helmet": "^3.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "zod": "^3.25.67",
    "zustand": "^5.0.6"
  }
}
